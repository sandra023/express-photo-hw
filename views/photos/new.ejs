<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <header>
      <h1>Add A Photo</h1>
      <nav>
        <ul>
          <li><a href="/">Home</a></li>
          <li><a href="/photos">Photo Index</a></li>
        </ul>
      </nav>
    </header>
    <main>
      <form action="/photos" method="post">
        <select name="userId">
          <%users.forEach(function(name,index){%>
              <option value="<%=users._id%>"><%=users.name%></option>
          <%})%>
        </select><br/>
        Title: <input type="text" name="title" /><br/>
        URl: <input type="text" name="url" /><br/>
        <input type="submit" value="Add Photo!"/>
      </form>
    </main>
  </body>
</html>

